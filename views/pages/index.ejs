<?xml version="1.0" encoding="UTF-8"?>
<!doctype html>
<html ng-app="HojeEhDiaDeRuaApp">
  <head>
      <title>Hoje é Dia de Rua</title>
      <meta charset="utf-8" />
      <link rel="stylesheet" href="res/style/diaderua.css" />
  </head>
  <body ng-controller="mainController" ng-init="init()">
    <div>
      <h1>{{title}}</h1>
      <h2><a href="estatisticas.html">Estatísticas</a></h2>
      <div id="corpo">
        <calendario dia="dia" mes="mes" />
        <localidades cidades="{{localidades}}" />
