    </div>
      <!-- vendors:js -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ngStorage/0.3.10/ngStorage.min.js"></script>
    <!-- endinject -->
    <!-- app:js -->
    <script src="res/script/app.js"></script>
    <script src="res/script/main.js"></script>
    <script src="res/script/filters.js"></script>
    <script src="res/script/directives.js"></script>
    <script src="res/script/calendario.js"></script>
    <script src="res/script/localidades.js"></script>
    <script src="res/script/localidade.js"></script>
    <!-- endinject -->
  </body>
</html>
